#################
#!/bin/bash

LIMIT=3D"1000" ## Suspend if battery level drops below this (in mAh/m=
Wh)
SLEEP=3D"60"  ## Seconds between each battery level check
BAT=3D"BAT0" ## Part of path: /proc/acpi/battery/BAT/
HIBERNATE=3D"/etc/acpi/hibernate.sh" ## command used to suspend
#HIBERNATE=3D"echo 4 > /proc/acpi/sleep"

while [ true ]; do
  if [ -e "/proc/acpi/battery/$BAT/state" ]; then
     PRESENT=3D$(/bin/sed -ne "/present:/{s/^present:[
]*\([a-z]*\)$/\1/p;q}" /proc/acpi/battery/$BAT/state)
     #echo $PRESENT
     if [ "$PRESENT" =3D "yes" ]; then
        STATE=3D$(/bin/sed -ne "/charging state:/{s/^charging state:[
]*\([a-zA-Z]*\)$/\1/p;q}" /proc/acpi/battery/$BAT/state)
        BATTERY=3D$(/bin/sed -ne "/remaining capacity:/{s/^remaining
capacity:[ ]*\([0-9]*\) m[WA]h$/\1/p;q}" /proc/acpi/battery/$BAT/stat=
e)
#        echo $BATTERY
#        echo $STATE
        if [ "$BATTERY" -lt "$LIMIT" ] && [ "$STATE" =3D "discharging=
" ];
then

           ## Comment out the following line if you don't
           ## want to log the event to system log:
           logger "Battery at ${BATTERY} mWh. Suspending to disk."
           #echo "Battery at ${BATTERY} mWh. Suspending to disk."

           ## Suspend:
           exec "$HIBERNATE"
       fi
     fi
  fi
  sleep ${SLEEP}s
done
